{"ast":null,"code":"import { getRoleList, roleDelete } from '../../services/systemManage';\nimport addRolepopUp from '@/components/permissManage/addRolepopUp.vue';\nimport PermissionsTree from '@/components/permissManage/permissionsTree.vue';\nexport default {\n  data() {\n    return {\n      //存储角色的列表 \n      rolesData: [],\n      roleId: 0,\n      showTree: false\n    };\n  },\n  components: {\n    addRolepopUp,\n    PermissionsTree\n  },\n  mounted() {\n    //获取角色列表 \n    this.getRoles();\n  },\n  methods: {\n    //获取角色列表\n    getRoles() {\n      getRoleList().then(res => {\n        if (res.data.code === 200) {\n          this.rolesData = res.data.data;\n        } else if (res.data.code === 403) {\n          this.$message.error(\"权限不足！！！\");\n        }\n      });\n    },\n    //添加角色 \n    addRole() {\n      this.$refs.addRolepopUp.showDialogVisible();\n    },\n    //编辑角色 \n    editRole(item) {\n      this.showTree = true;\n      this.roleId = item.roleId;\n      this.$refs.PermissionsTree.showTreeList(item.roleId, [], item.name);\n    },\n    //删除角色 \n    deleteRole(roleId) {\n      roleDelete(roleId).then(res => {\n        console.log(res);\n        if (res.data.code === 200) {\n          this.$message.success(\"删除角色成功！\");\n          this.getRoles();\n        } else if (res.data.code === 400) {\n          this.$message.error(\"无法删除该角色，该角色已经被分配给用户，请取消分配再删除!!\");\n        }\n      });\n    },\n    //编辑权限树\n    editPermission() {\n      //调用方法传入-1,标明知识查看权限树 \n      this.$refs.PermissionsTree.showTreeList(-1, [], '');\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA;AACA;AACA;AACA;EACAA;IACA;MACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;IACA;EACA;EACAC;IACA;IACAC;MACAC;QACA;UACA;QACA;UACA;QACA;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACA;IACAC;MACAC;QACAC;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEA;IACAC;MACA;MACA;IACA;EAEA;AACA","names":["data","rolesData","roleId","showTree","components","addRolepopUp","PermissionsTree","mounted","methods","getRoles","getRoleList","addRole","editRole","deleteRole","roleDelete","console","editPermission"],"sourceRoot":"src/views/systemManage","sources":["permissManage.vue"],"sourcesContent":["<!--权限管理-->\r\n<template>\r\n  <div class=\"appMain\">\r\n    <div class=\"filter-container\">\r\n      <div class=\"fillter-container-BtnList\">\r\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"addRole\" v-has=\"`system:role:add`\">添加角色</el-button>\r\n        <el-button type=\"warning\" icon=\"el-icon-edit\" @click=\"editPermission\">编辑权限</el-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"filter-primissList\">\r\n      <div class=\"primissList\">\r\n        <div v-for=\"(item) in rolesData\" :key=\"item.roleId\" class=\"roleFont roleItem\">\r\n          <div class=\"itemInfo\">\r\n            {{ item.name }}\r\n          </div>\r\n          <div class=\"itemBtn\">\r\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-edit\" circle @click=\"editRole(item)\"\r\n              v-has=\"`system:role:getMenu`\"></el-button>\r\n            <el-button type=\"primary\" size=\"mini\" icon=\"el-icon-delete\" circle v-if=\"item.roleId > 4\"\r\n              @click=\"deleteRole(item.roleId)\" v-has=\"`system:role:delete`\"></el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"primissTr\">\r\n        <permissionsTree ref=\"PermissionsTree\"></permissionsTree>\r\n      </div>\r\n\r\n    </div>\r\n    <addRolepopUp ref=\"addRolepopUp\"></addRolepopUp>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getRoleList, roleDelete } from '../../services/systemManage'\r\nimport addRolepopUp from '@/components/permissManage/addRolepopUp.vue'\r\nimport PermissionsTree from '@/components/permissManage/permissionsTree.vue';\r\nexport default {\r\n  data() {\r\n    return {\r\n      //存储角色的列表 \r\n      rolesData: [],\r\n      roleId: 0,\r\n      showTree: false,\r\n    };\r\n  },\r\n  components: {\r\n    addRolepopUp,\r\n    PermissionsTree,\r\n  },\r\n  mounted() {\r\n    //获取角色列表 \r\n    this.getRoles()\r\n  },\r\n  methods: {\r\n    //获取角色列表\r\n    getRoles() {\r\n      getRoleList().then((res) => {\r\n        if (res.data.code === 200) {\r\n          this.rolesData = res.data.data;\r\n        } else if (res.data.code === 403) {\r\n          this.$message.error(\"权限不足！！！\")\r\n        }\r\n      })\r\n    },\r\n    //添加角色 \r\n    addRole() {\r\n      this.$refs.addRolepopUp.showDialogVisible()\r\n    },\r\n    //编辑角色 \r\n    editRole(item) {\r\n      this.showTree = true;\r\n      this.roleId = item.roleId\r\n      this.$refs.PermissionsTree.showTreeList(item.roleId, [], item.name)\r\n    },\r\n    //删除角色 \r\n    deleteRole(roleId) {\r\n      roleDelete(roleId).then((res) => {\r\n        console.log(res);\r\n        if (res.data.code === 200) {\r\n          this.$message.success(\"删除角色成功！\")\r\n          this.getRoles()\r\n        } else if (res.data.code === 400) {\r\n          this.$message.error(\"无法删除该角色，该角色已经被分配给用户，请取消分配再删除!!\")\r\n        }\r\n      })\r\n    },\r\n\r\n    //编辑权限树\r\n    editPermission() {\r\n      //调用方法传入-1,标明知识查看权限树 \r\n      this.$refs.PermissionsTree.showTreeList(-1, [], '')\r\n    }\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.appMain {\r\n  padding: 20px 5px 50px 15px;\r\n  display: block;\r\n\r\n  .filter-container {\r\n    margin-left: 10px;\r\n    width: 290px;\r\n\r\n    .fillter-container-BtnList {\r\n      display: flex;\r\n      justify-content: space-between;\r\n    }\r\n  }\r\n}\r\n\r\n.filter-primissList {\r\n  display: flex;\r\n\r\n  .primissList {\r\n    width: 25rem;\r\n    border-right: 1px solid #9093994a;\r\n\r\n    .roleItem {\r\n      display: flex;\r\n      margin: 5px 10px;\r\n      padding: 15px 0;\r\n      align-items: center;\r\n      justify-content: space-between;\r\n    }\r\n\r\n    .roleFont {\r\n      font-size: 16px;\r\n      background-color: #409eff38;\r\n      border-color: #e9e9eb;\r\n      color: black;\r\n      border-radius: 10px;\r\n    }\r\n  }\r\n  .primissTr{\r\n    width: 100%;\r\n    margin: 0 a;\r\n  }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n.itemInfo {\r\n  margin-left: 20px;\r\n}\r\n\r\n.itemBtn {\r\n  margin-right: 15px;\r\n}\r\n\r\n\r\n.selectBox {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n  clear: both\r\n}\r\n\r\n.box-card {\r\n  width: 360px;\r\n\r\n  /deep/.el-card__body {\r\n    padding: 10px 30px;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}